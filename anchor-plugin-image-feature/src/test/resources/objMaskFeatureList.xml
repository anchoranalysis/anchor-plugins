<config>
<bean config-class="org.anchoranalysis.feature.bean.list.FeatureListProviderDefine">
	<list config-class="java.util.List" config-factory="list">
		<item config-class="org.anchoranalysis.image.feature.bean.object.single.CenterOfGravity" axis="y"/>
		<item config-class="org.anchoranalysis.plugin.image.feature.object.calculation.single.boundingbox.BoundingBoxMaximumAlongAxis" axis="x"/>
		<item config-class="org.anchoranalysis.plugin.image.feature.object.calculation.single.boundingbox.BoundingBoxMinimumAlongAxis" axis="z"/>
		<item config-class="org.anchoranalysis.plugin.image.feature.bean.object.single.intensity.Intensity" nrgIndex="0"/>
		<item config-class="org.anchoranalysis.plugin.image.feature.bean.object.single.intensity.IntensityMeanShell" nrgIndex="1" iterationsDilation="2"/>
		<item config-class="org.anchoranalysis.image.feature.bean.object.single.NumberVoxels"/>
		<item config-class="ch.ethz.biol.cell.mpp.nrg.feature.objmask.NumBorderVoxels"/>
	</list>
</bean>
</config>