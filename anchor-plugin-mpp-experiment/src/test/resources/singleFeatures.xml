<config>
<bean config-class="java.util.List" config-factory="list">

	<item config-class="org.anchoranalysis.bean.NamedBean" name="numVoxels">
		<item config-class="org.anchoranalysis.feature.bean.list.FeatureListProviderDefineSingle">
			<item config-class="org.anchoranalysis.image.feature.bean.object.single.NumberVoxels"/>
		</item>
	</item>

	<item config-class="org.anchoranalysis.bean.NamedBean" name="cogX">
		<item config-class="org.anchoranalysis.feature.bean.list.FeatureListProviderDefineSingle">
			<item config-class="org.anchoranalysis.image.feature.bean.object.single.CenterOfGravity" axis="y"/>
		</item>
	</item>

	<item config-class="org.anchoranalysis.bean.NamedBean" name="cogY">
		<item config-class="org.anchoranalysis.feature.bean.list.FeatureListProviderDefineSingle">
			<item config-class="org.anchoranalysis.image.feature.bean.object.single.CenterOfGravity" axis="y"/>
		</item>
	</item>

	<item config-class="org.anchoranalysis.bean.NamedBean" name="numBorderVoxels">
		<item config-class="org.anchoranalysis.feature.bean.list.FeatureListProviderDefineSingle">
			<item config-class="org.anchoranalysis.plugin.image.feature.bean.object.single.border.NumberVoxelsAtBorder"/>
		</item>
	</item>
	
	<item config-class="org.anchoranalysis.bean.NamedBean" name="meanIntensity">
		<item config-class="org.anchoranalysis.feature.bean.list.FeatureListProviderDefineSingle">
			<item config-class="org.anchoranalysis.plugin.image.feature.bean.object.single.intensity.Intensity" energyIndex="0"/>
		</item>
	</item>
	
</bean>
</config>